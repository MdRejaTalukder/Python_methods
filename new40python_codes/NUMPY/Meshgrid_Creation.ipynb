   "source": [
    "# Example 18: NumPy - Meshgrid Creation\n",
    "import numpy as np\n",
    "\n",
    "# Creating 1D arrays\n",
    "x = np.array([1, 2, 3])\n",
    "y = np.array([4, 5, 6])\n",
    "\n",
    "# Creating a meshgrid\n",
    "X, Y = np.meshgrid(x, y)\n",
    "\n",
    "print('Meshgrid X:\\n', X)\n",
    "print('Meshgrid Y:\\n', Y)"
   ]
  }
