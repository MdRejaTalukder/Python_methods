    "# Example 10: NumPy - Sorting Arrays\n",
    "import numpy as np\n",
    "\n",
    "# Creating an array\n",
    "array = np.array([3, 1, 2, 5, 4])\n",
    "\n",
    "# Sorting the array\n",
    "sorted_array = np.sort(array)\n",
    "\n",
    "print('Original Array:', array)\n",
    "print('Sorted Array:', sorted_array)"
   ]
