    "# Example 21: NumPy - Reverse Array using Indexing\n",
    "import numpy as np\n",
    "\n",
    "# Creating an array\n",
    "array = np.array([1, 2, 3, 4, 5])\n",
    "\n",
    "# Reversing the array using slicing\n",
    "reversed_array = array[::-1]\n",
    "\n",
    "print('Reversed Array:', reversed_array)"
   ]
  }
